lmao.Name = "lmao"
lmao.Parent = game.CoreGui
lmao.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

top.Name = "top"
top.Parent = lmao
top.Active = true
top.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
top.BorderSizePixel = 0
top.Position = UDim2.new(0.098413676, 0, 0.244057059, 0)
top.Size = UDim2.new(0, 300, 0, 40)

minimize.Name = "minimize"
minimize.Parent = top
minimize.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
minimize.BorderSizePixel = 0
minimize.Size = UDim2.new(0, 50, 0, 40)
minimize.Font = Enum.Font.GothamBold
minimize.Text = "-"
minimize.TextColor3 = Color3.fromRGB(255, 255, 0)
minimize.TextSize = 40.000
minimize.MouseButton1Click:Connect(function()
	if main.Visible == true then
		main.Visible = false
	else
		if main.Visible == false then
			main.Visible = true
		end
	end
end)

